{
  "name": "truck-tracker",
  "version": "1.0.0",
  "description": "A truck logging and tracking app for Android and iOS using React Native Expo",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "setup": "npm install && echo 'Dependencies installed successfully!'",
    "clean": "rm -rf node_modules package-lock.json && echo 'Dependencies cleaned!'",
    "fresh": "npm run clean && npm run setup"
  },
  "dependencies": {
    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "react-native-screens": "~3.22.0",
    "react-native-safe-area-context": "4.6.3",
    "expo-camera": "~13.4.4",
    "expo-image-picker": "~14.3.2",
    "react-native-paper": "^5.10.6",
    "react-native-vector-icons": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
